{
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.10.0",
  "scripts": {
    "clean": "node scripts/clean.mjs",
    "build": "tsc -b tsconfig.build.json && pnpm --recursive --parallel run build",
    "check": "pnpm --recursive exec tsc -b tsconfig.json",
    "lint": "biome check",
    "format": "biome check --fix",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "changeset:create": "changeset",
    "changeset:version": "biome check && changeset version && biome check --fix",
    "changeset:publish": "pnpm build && TEST_DIST= pnpm vitest && changeset publish",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/cli": "8.0.0-beta.3",
    "@babel/core": "8.0.0-beta.3",
    "@babel/plugin-transform-export-namespace-from": "8.0.0-beta.3",
    "@babel/plugin-transform-modules-commonjs": "8.0.0-beta.3",
    "@biomejs/biome": "2.3.8",
    "@changesets/changelog-github": "0.5.2",
    "@changesets/cli": "2.29.8",
    "@effect/build-utils": "0.8.9",
    "@effect/language-service": "0.57.1",
    "@effect/platform": "0.93.5",
    "@effect/vitest": "0.27.0",
    "@types/node": "24.10.1",
    "effect": "3.19.8",
    "glob": "13.0.0",
    "husky": "9.1.7",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "vitest": "4.0.14"
  }
}
