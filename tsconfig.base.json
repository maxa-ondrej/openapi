{
  "compilerOptions": {
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "moduleDetection": "force",
    "composite": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "esModuleInterop": false,
    "declaration": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "NodeNext",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "types": [],
    "isolatedModules": true,
    "sourceMap": true,
    "declarationMap": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": false,
    "noErrorTruncation": false,
    "allowJs": false,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": false,
    "strictNullChecks": true,
    "baseUrl": ".",
    "target": "ES2022",
    "module": "NodeNext",
    "incremental": true,
    "removeComments": false,
    "plugins": [{ "name": "@effect/language-service" }],
    "paths": {
      "@majksa-openapi/effect": ["./packages/effect/src/index.js"],
      "@majksa-openapi/effect/*": ["./packages/effect/src/*.js"],
      "@majksa-openapi/effect/test/*": ["./packages/effect/test/*.js"],
      "@majksa-openapi/effect-heyapi-plugin": [
        "./packages/effect-heyapi-plugin/src/index.js"
      ],
      "@majksa-openapi/effect-heyapi-plugin/*": [
        "./packages/effect-heyapi-plugin/src/*.js"
      ],
      "@majksa-openapi/effect-heyapi-plugin/test/*": [
        "./packages/effect-heyapi-plugin/test/*.js"
      ]
    }
  }
}
